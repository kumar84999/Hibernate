<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FindAllController</servlet-name>
  	<display-name>FindAllController</display-name>
  	<description></description>
  	<servlet-class>com.controller.FindAllController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertController</servlet-name>
  	<display-name>InsertController</display-name>
  	<description></description>
  	<servlet-class>com.controller.InsertController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateController</servlet-name>
  	<display-name>UpdateController</display-name>
  	<description></description>
  	<servlet-class>com.controller.UpdateController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteController</servlet-name>
  	<display-name>DeleteController</display-name>
  	<description></description>
  	<servlet-class>com.controller.DeleteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetByIdController</servlet-name>
  	<display-name>GetByIdController</display-name>
  	<description></description>
  	<servlet-class>com.controller.GetByIdController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FindAllController</servlet-name>
  	<url-pattern>/findall</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertController</servlet-name>
  	<url-pattern>/insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateController</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteController</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetByIdController</servlet-name>
  	<url-pattern>/getbyid</url-pattern>
  </servlet-mapping>
</web-app>
